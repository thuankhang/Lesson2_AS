<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/denim"
        tools:context=".MainActivity">



    <TextView
            android:layout_width="@dimen/screen_signup_width"
            android:layout_height="@dimen/screen_signup_height"
            android:text="@string/screen_signup"
            android:textColor="@color/white"
            android:textAlignment="center"
            android:textSize="@dimen/screen_signup_size"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="@dimen/margin"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginEnd="@dimen/margin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="@dimen/margin"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.045"
            android:id="@+id/textView2"
    />

    <TextView
            android:text="@string/screen_who"
            android:textColor="@color/stone"
            android:textSize="@dimen/screen_who_size"
            android:textAlignment="center"
            android:layout_width="@dimen/screen_who_width"
            android:layout_height="@dimen/screen_who_height"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.032"
    />

    <ImageButton
            android:background="@color/transparent"
            android:layout_width="@dimen/image_button_width"
            android:layout_height="@dimen/image_button_height"
            app:srcCompat="@mipmap/ic_launcher_parent"
            android:id="@+id/imageButton"
            tools:layout_editor_absoluteY="@dimen/image_button_absoluteY"
            app:layout_constraintEnd_toStartOf="@+id/imageButton2"
    />

    <ImageButton
            android:background="@color/transparent"
            android:layout_width="@dimen/image_button_width"
            android:layout_height="@dimen/image_button_height"
            app:srcCompat="@mipmap/ic_launcher_child"
            android:id="@+id/imageButton2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            tools:layout_editor_absoluteY="@dimen/image_button_absoluteY"
    />

    <ImageButton
            android:background="@color/transparent"
            android:layout_width="@dimen/image_button_width"
            android:layout_height="@dimen/image_button_height"
            app:srcCompat="@mipmap/ic_launcher_teacher"
            tools:layout_editor_absoluteY="@dimen/image_button_absoluteY"
            android:id="@+id/imageButton3"
            app:layout_constraintStart_toEndOf="@+id/imageButton2"
    />

    <TextView
            android:text="@string/screen_parent"
            android:textColor="@color/biscotti"
            android:textAlignment="center"
            android:layout_width="@dimen/parent_child_teacher_width"
            android:layout_height="@dimen/parent_child_teacher_height"
            tools:layout_editor_absoluteX="@dimen/parent_absoluteX"
            android:id="@+id/textView6"
            tools:layout_editor_absoluteY="@dimen/parent_child_teacher_absoluteY"
    />

    <TextView
            android:text="@string/screen_child"
            android:textColor="@color/tangerine"
            android:textAlignment="center"
            android:layout_width="@dimen/parent_child_teacher_width"
            android:layout_height="@dimen/parent_child_teacher_height"
            android:id="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            tools:layout_editor_absoluteY="@dimen/parent_child_teacher_absoluteY"
    />

    <TextView
            android:text="@string/screen_teacher"
            android:textAlignment="center"
            android:textColor="@color/teal"
            android:layout_width="@dimen/parent_child_teacher_width"
            android:layout_height="@dimen/parent_child_teacher_height"
            tools:layout_editor_absoluteX="@dimen/teacher_absoluteX"
            android:id="@+id/textView8"
            tools:layout_editor_absoluteY="@dimen/parent_child_teacher_absoluteY"
    />

    <TextView
            android:text="@string/screen_user"
            android:textColor="@color/stone"
            android:background="@drawable/contour"
            android:textAlignment="center"
            android:textSize="@dimen/screen_uepcp_size"
            android:layout_width="@dimen/screen_uepcp_width"
            android:layout_height="@dimen/screen_uepcp_height"
            android:id="@+id/textView10"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.438"
    />

    <TextView
            android:text="@string/screen_email"
            android:textColor="@color/stone"
            android:background="@drawable/contour"
            android:textAlignment="center"
            android:textSize="@dimen/screen_uepcp_size"
            android:layout_width="@dimen/screen_uepcp_width"
            android:layout_height="@dimen/screen_uepcp_height"
            android:id="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.533"
    />

    <TextView
            android:text="@string/screen_pass"
            android:textColor="@color/stone"
            android:background="@drawable/contour"
            android:textAlignment="center"
            android:textSize="@dimen/screen_uepcp_size"
            android:layout_width="@dimen/screen_uepcp_width"
            android:layout_height="@dimen/screen_uepcp_height"
            android:id="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.626"
    />

    <TextView
            android:text="@string/screen_confirmpass"
            android:textColor="@color/stone"
            android:background="@drawable/contour"
            android:textAlignment="center"
            android:textSize="@dimen/screen_uepcp_size"
            android:layout_width="@dimen/screen_uepcp_width"
            android:layout_height="@dimen/screen_uepcp_height"
            android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.722"
    />

    <ImageView
            android:layout_width="@dimen/screen_icon"
            android:layout_height="@dimen/screen_icon"
            app:srcCompat="@mipmap/ic_launcher_user"
            android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="@+id/textView10"
            app:layout_constraintTop_toTopOf="@+id/textView10"
            app:layout_constraintBottom_toBottomOf="@+id/textView10"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginLeft="@dimen/margin_icon"
            android:layout_marginStart="@dimen/margin_icon"
    />

    <ImageView
            android:layout_width="@dimen/screen_icon"
            android:layout_height="@dimen/screen_icon"
            app:srcCompat="@mipmap/ic_launcher_email"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            android:layout_marginLeft="@dimen/margin_icon"
            android:layout_marginStart="@dimen/margin_icon"
            app:layout_constraintTop_toTopOf="@+id/textView3"
            app:layout_constraintBottom_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.0"
    />

    <ImageView
            android:layout_width="@dimen/screen_icon"
            android:layout_height="@dimen/screen_icon"
            app:srcCompat="@mipmap/ic_launcher_lock"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/textView11"
            android:layout_marginLeft="@dimen/margin_icon"
            android:layout_marginStart="@dimen/margin_icon"
            app:layout_constraintTop_toTopOf="@+id/textView11"
            app:layout_constraintBottom_toBottomOf="@+id/textView11"
            app:layout_constraintVertical_bias="0.0"
    />

    <ImageView
            android:layout_width="@dimen/screen_icon"
            android:layout_height="@dimen/screen_icon"
            app:srcCompat="@mipmap/ic_launcher_lock"
            android:id="@+id/imageView4"
            app:layout_constraintStart_toStartOf="@+id/textView9"
            android:layout_marginLeft="@dimen/margin_icon"
            android:layout_marginStart="@dimen/margin_icon"
            app:layout_constraintTop_toTopOf="@+id/textView9"
            app:layout_constraintBottom_toBottomOf="@+id/textView9"
            app:layout_constraintVertical_bias="0.0"
    />

    <Button
            android:text="@string/button_signup"
            android:textColor="@color/white"
            android:layout_width="@dimen/button_signup_width"
            android:layout_height="@dimen/button_signup_height"
            android:background="@drawable/signup_background"
            android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toTopOf="@+id/textView4"
    />


    <TextView
            android:text="@string/screen_login"
            android:textColor="@color/white"
            android:layout_width="@dimen/screen_login_width"
            android:layout_height="@dimen/screen_login_height"
            android:id="@+id/textView4"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="@dimen/margin"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            app:layout_constraintVertical_bias="0.963"
            app:layout_constraintHorizontal_bias="0.37"
    />

    <TextView
            android:text="@string/button_login"
            android:textColor="@color/cider"
            android:layout_width="@dimen/button_login_width"
            android:layout_height="@dimen/button_login_height"
            android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/margin"
            android:layout_marginStart="@dimen/margin"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/margin"
            android:layout_marginRight="@dimen/margin"
            android:layout_marginBottom="@dimen/margin"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="@dimen/margin"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.767"
            app:layout_constraintVertical_bias="0.963"
    />



</androidx.constraintlayout.widget.ConstraintLayout>